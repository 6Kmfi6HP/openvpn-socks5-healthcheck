# Container name
NAME=vpn-socks5

# SOCKS5 proxy configuration
SOCKS5_USER=user
SOCKS5_PASS=pass
SOCKS5_PORT=7777

# VPN configuration
ACTIVE_CONFIG=
VPN_CONFIGS_DIR=/vpn

# VPN switching configuration
# Mode can be "time" or "health"
VPN_SWITCH_MODE=health
# Interval for time-based switching (in seconds, default: 1 hour)
VPN_SWITCH_INTERVAL=3600
# Health check configuration
HEALTH_CHECK_INTERVAL=60
HEALTH_CHECK_URL=https://www.google.com
HEALTH_CHECK_TIMEOUT=10
HEALTH_CHECK_RETRIES=3
HEALTH_CHECK_RETRY_INTERVAL=2

# Update configuration
UPDATE_INTERVAL=24  # VPN configs update interval in hours 